<%= form_for(@kitten, url: kitten_new_path) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :age %>
    <%= f.text_field :age %>
    <br>
    <%= f.label :cuteness %>
    <%= f.text_field :cuteness %>
    <br>
    <%= f.label :softness %>
    <%= f.text_field :softness %>
    <br>
    <% provide(:button_text, 'Create') %>
    <% f.submit "Create kitty" %>
<% end%>